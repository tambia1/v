{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"compounds": [
		{
			"name": "Run project",
			"configurations": ["Launch Edge 8080", "Run dev"],
			"stopAll": true
		}
	],
	"configurations": [
		{
			"name": "Launch Edge 8080",
			"type": "msedge",
			"request": "launch",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}",
			"sourceMapPathOverrides": {
				"\\mnt\\c\\*": "C:/*"
			}
		},
		{
			"name": "Run install",
			"command": "npm i",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Run dev",
			"command": "npm run dev",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Run test",
			"command": "npm run test",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Run coverage",
			"command": "npm run coverage",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Run build",
			"command": "npm run build",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Run preview",
			"command": "npm run preview",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Run deploy",
			"command": "npm run deploy",
			"request": "launch",
			"type": "node-terminal"
		}
	]
}
