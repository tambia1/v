{
	"version": "0.2.0",
	"compounds": [
		{
			"name": "Run project",
			"configurations": ["launch brave 5000", "app start", "frontend preview", "websocket server", "graphql server", "redis server"],
			"stopAll": true
		}
	],
	"configurations": [
		{
			"name": "launch edge 5000",
			"type": "msedge",
			"request": "launch",
			"url": "http://localhost:5000",
			"webRoot": "${workspaceFolder}/app",
			"sourceMapPathOverrides": {
				"\\mnt\\c\\*": "C:/*"
			}
		},
		{
			"name": "launch brave 5000",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:5000",
			"webRoot": "${workspaceFolder}/app",
			"runtimeExecutable": "/usr/bin/brave-browser-stable",
			"sourceMapPathOverrides": {
				"\\mnt\\c\\*": "C:/*"
			},
			"sourceMaps": true
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "packages install",
			"command": "npm install --legacy-peer-deps",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/"
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "app install",
			"command": "npm i",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "app start",
			"command": "npm run start",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/",
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"name": "app test",
			"command": "npm run test",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "app coverage",
			"command": "npm run coverage",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "app build",
			"command": "npm run build",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "app preview",
			"command": "npm run preview",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "app deploy",
			"command": "npm run deploy",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "app release",
			"command": "npm run release",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/src/"
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "playwright test",
			"command": "npx playwright test",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright test --headed",
			"command": "npx playwright test --headed",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright test --ui",
			"command": "npx playwright test --ui",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright test --update-snapshots",
			"command": "npx playwright test --update-snapshots",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright browser",
			"command": "npx playwright test --project=chromium",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright test file",
			"command": "npx playwright test example",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright debug mode",
			"command": "npx playwright test --debug",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright codegen",
			"command": "npx playwright codegen http://localhost:5000/v",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright codegen har",
			"command": "npx playwright codegen http://localhost:5000/v --save-har aaa.har --viewport-size=430,932",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "playwright report",
			"command": "npx playwright show-report",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/"
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "app create v application",
			"command": "node createApp.js",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/scripts/"
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "frontend install",
			"command": "npm i",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/frontend/mfeTest/"
		},
		{
			"name": "frontend start",
			"command": "npm run start",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/frontend/mfeTest/"
		},
		{
			"name": "frontend build",
			"command": "npm run build",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/frontend/mfeTest/"
		},
		{
			"name": "frontend preview",
			"command": "npm run preview",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/frontend/mfeTest/"
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "websocket server",
			"command": "node WebSocketServer.js",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/src/pages/pageHome/components/desktop/apps/chat/server/"
		},
		{
			"name": "graphql server",
			"command": "node index.js",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/src/pages/pageHome/components/desktop/apps/testGraphQl/server/",
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"name": "redis server",
			"command": "node index.js",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/app/src/pages/pageHome/components/desktop/apps/testRedis/server/",
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "ui install",
			"command": "npm i",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/shared/ui/"
		},
		{
			"name": "ui start",
			"command": "npm run start",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/shared/ui/"
		},
		{
			"name": "ui build",
			"command": "npm run build",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}/shared/ui/"
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Update npm",
			"command": "npm install -g npm stable",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Update node",
			"command": "npm install -g node stable",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "Update VSCode",
			"command": "sudo apt update && sudo apt install code",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "---------------------------------",
			"command": "",
			"request": "launch",
			"type": "node-terminal"
		}
	]
}
