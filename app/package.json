        "@tanstack/react-query-devtools": "5.63.0",
        "@v/shared-ui": "file:../@v/shared-ui",
        "date-fns": "4.1.0",
        "graphql": "16.10.0",
        "https": "1.0.0",
        "i18next": "24.2.1",
        "i18next-browser-languagedetector": "8.0.2",
        "localforage": "1.10.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-i18next": "15.4.0",
        "react-modal": "3.16.3",
        "react-router-dom": "7.1.1",
        "react-svg": "16.2.0",
        "redis": "4.7.0",
        "styled-components": "6.1.14",
        "zod": "3.24.1",
        "zustand": "5.0.3"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@originjs/vite-plugin-federation": "1.3.7",
        "@playwright/test": "1.49.1",
